{
  "name": "koa-experiment",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "node index.js",
    "clean": "rm -rf ./build",
    "build": "npm run clean && webpack --env.prod",
    "client": "webpack-dev-server --mode development --devtool inline-source-map --hot",
    "server": "nodemon ./index.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "postinstall": "npm run build"
  },
  "dependencies": {
    "@koa/cors": "^2.2.1",
    "cross-env": "^5.1.6",
    "dotenv": "^5.0.1",
    "koa": "^2.5.1",
    "koa-bodyparser": "^4.2.1",
    "koa-helmet": "^4.0.0",
    "koa-logger": "^3.2.0",
    "koa-respond": "^1.0.1",
    "koa-router": "^7.4.0",
    "koa-static": "^4.0.3",
    "koa-views": "^6.1.4",
    "mongoose": "^5.1.2",
    "react": "^16.4.0",
    "react-dom": "^16.4.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "concurrently": "^3.5.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "nodemon": "^1.17.5",
    "webpack": "^4.9.1",
    "webpack-cli": "^2.1.4",
    "webpack-dev-server": "^3.1.4"
  }
}
